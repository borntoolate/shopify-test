    "stylelint-scss": "^4.3.0",
    "svg-sprite-loader": "^6.0.11",
    "svg-transform-loader": "^2.0.13",
    "svgo": "^2.8.0",
    "svgo-loader": "^3.0.1",
    "ts-jest": "^29.0.3",
